{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Jest All",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "args": ["--runInBand"]
    },
    {
      "name": "Jest This",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "args": [
        "--runInBand",
        "${file}"
      ]
    },
    {
      "name": "Jest Day",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "JEST_DAY": "FOO${file}"
      },
      "type": "pwa-node",
      "args": [
        "--runInBand",
        "${workspaceFolder}/test/days.test.js"
      ],
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Current File",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${file}"
    }
  ]
}
